###
### Fichier de configuration du client
###

#Extension des fichiers temporaires
tmpExtension = .tmp

#Répertoire contenant les fichiers temporaires
tmpDir = tmp

#Répertoire contetnant les fichiers complets
downloadDir = downloads

#Taille des pièces (ne pas modifier)
pieceSize = 2048

#Timeout en miliseconde des communication clientes sockets
socketClientTimeout = 60000

#Timeout en miliseconde des communication serveurs sockets
socketServerTimeout = 60000

#Port d'écoute des autres pairs
listenPort = 60000

#Port découte du tracker
trackerPort = 6000

#Adresse IP du tracker
trackerIP = 127.0.0.1

#Nombre de pieces téléchargés à la fois au maximum
maxDownloadedPieces = 4

